mixin slider-thumbnav(length, imgSrc = `${assets}slider-thumb-img-`)
	ul.thumbnav&attributes(attributes)
		if block 
			block 
		else
			- for (var i = 0; i < length; i++)
				li.thumbnav__item(data-uk-slider-item=i)
					a.thumbnav__link.ibg(href="#")
						+picture(jpg, `${imgSrc}${i + 1}`, 'n')

mixin slideshow-thumbnav(length, imgSrc = `${assets}slider-thumb-img-`)
	ul.thumbnav&attributes(attributes)
		if block 
			block 
		else
			- for (var i = 0; i < length; i++)
				li.thumbnav__item(data-uk-slideshow-item=i)
					a.thumbnav__link.ibg(href="#")
						+picture(jpg, `${imgSrc}${i + 1}`, 'n')



