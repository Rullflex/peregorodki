include widgets/_header
include widgets/_footer
include widgets/_menu


//- BREADCRUMB
mixin breadcrumb(arrayOfLink = [], readTime)
	ul.breadcrumb&attributes(attributes)
		.breadcrumb__readtime!= readTime
		each item, idx in arrayOfLink
			if idx == arrayOfLink.length - 1
				li.breadcrumb__item.breadcrumb__item--last
					span.breadcrumb__link!= item
			else
				li.breadcrumb__item
					a.breadcrumb__link(href="#")= item
mixin breadcrumb-bot(arrayOfLink = [], readTime)
	ul.breadcrumb.breadcrumb--bot&attributes(attributes)
		//- .breadcrumb__readtime!= readTime
		each item, idx in arrayOfLink
			li.breadcrumb__item
				a.breadcrumb__link(href="#")!= item
//- PLAY BTN
mixin play-btn(text, vhref = "#video")
	.play-btn&attributes(attributes)
		a.play-btn__btn(href=vhref data-uk-toggle="")
		.play-btn__text 
			.play-btn__text-p!= text


//- POPUP BASE

mixin popup
	.popup.uk-flex-top(data-uk-modal)&attributes(attributes)
		.popup__body.uk-modal-dialog.uk-modal-body.uk-margin-auto-vertical
			button.popup__close.uk-modal-close-default(type="button" data-uk-close)
			block

mixin popup-video-16-9
	.popup-video.popup.uk-flex-top(data-uk-modal)&attributes(attributes)
		.popup__body.uk-modal-dialog.uk-modal-body.uk-margin-auto-vertical.embed-responsive.embed-responsive-16by9
			button.popup__close.uk-modal-close-default(type="button" data-uk-close)


//- SLIDER

mixin slider-nav
	.slider-nav
		ul.dotnav.uk-slider-nav&attributes(attributes)
		ul.slidenav&attributes(attributes)
			li.slidenav__item(data-uk-slider-item="previous")
				a.slidenav__link.slidenav__link--prev(href="#")
					+icon('angle-left').slidenav__icon
			li.slidenav__item(data-uk-slider-item="next")
				a.slidenav__link.slidenav__link--next(href="#")
					+icon('angle-right').slidenav__icon

mixin slideshow-nav
	.slideshow-nav
		ul.dotnav.uk-slideshow-nav&attributes(attributes)
		ul.slidenav&attributes(attributes)
			li.slidenav__item(data-uk-slideshow-item="previous")
				a.slidenav__link.slidenav__link--prev(href="#")
					+icon('angle-left').slidenav__icon
			li.slidenav__item(data-uk-slideshow-item="next")
				a.slidenav__link.slidenav__link--next(href="#")
					+icon('angle-right').slidenav__icon

mixin slider-dotnav
	ul.dotnav.uk-slider-nav&attributes(attributes)

mixin slideshow-dotnav
	ul.dotnav.uk-slideshow-nav&attributes(attributes)


mixin slider-slidenav
	ul.slidenav&attributes(attributes)
		li.slidenav__item(data-uk-slider-item="previous")
			a.slidenav__link.slidenav__link--prev(href="#")
				+icon('angle-left').slidenav__icon
		li.slidenav__item(data-uk-slider-item="next")
			a.slidenav__link.slidenav__link--next(href="#")
